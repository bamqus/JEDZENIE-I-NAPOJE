<?XML version="1.0" encoding="UTF-8"?>
    <slodkosci>

        <kremowka>
            <czas="1h" piekarniktemp="180C"></czas>
            <maka="pszenna" marka="Polskie Młyny"></maka>
            <cukier="biały" ilosc="180g"></cukier>
            <mleko="krowie" tluszcz="2%"></mleko>
        </kremowka>

        <murzynek>
            <czas="2h" piekarniktemp="180C"></czas>
            <czekolada="gorzka" procent="96%"></czekolada>
            <cukier="brązowy" ilosc="100g"></cukier>
            <proszek="do pieczenia" ilosc="2tsp"></proszek>
        </murzynek>

        <sernik>
            <czas="30min" pierkarniktemp="180C"></czas>
            <ser="Twaróg" rodzaj="chudy"></ser>
            <galaretka="Truskawkowa" marka="Dr. Oekter"></galaretka>
            <owoce="maliny" ilosc="500g"></owoce>

        </sernik>

        <kakao>
            <czas="5min" mikrofalowka="1000w"></czas>
            <proszek="czekoladowy" marka="Nesqucick"></proszek>
            <mleko="krowie" tluszcz="2%"></mleko>
            <dodatki="pianki" ilosc="garść"></dodatki>


        </kakao>


        <koktajl>
            <czas="5min" blender="200W"></czas>
            <owoce="Truskawki" ilosc="500g"></owoce>
            <cukier="brązowy" ilosc="2 łyżeczki"></cukier>
            <nabial="kefir" rodzaj="mleczny"></nabial>
        </koktajl>

        <pancake>
            <czas="10min" patelnia="20C"></czas>
            <owoce="Truskawki" ilosc="20g"></owoce>
            <cukier="brązowy" ilosc="4 lyzeczki"></cukier>
            <nabial="mleko" rodzaj="krowi"></nabial>
        </pancake>


    </slodkosci>
